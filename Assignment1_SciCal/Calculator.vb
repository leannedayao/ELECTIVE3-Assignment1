Public Class Calculator

    Private Sub Zero_Click(sender As Object, e As EventArgs) Handles Zero.Click
        If Display.Text = "0" Then
            Display.Text = "0"

        Else
            Display.Text = Display.Text + "0"
        End If
    End Sub

